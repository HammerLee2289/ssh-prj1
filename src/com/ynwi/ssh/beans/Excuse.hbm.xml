<?xml version="1.0" encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >  
  
<hibernate-mapping package="com.ynwi.ssh.beans">  
    <class name="Excuse" table="excuse">  
        <id name="excuseId" column="excuseId">  
            <generator class="native"></generator>  
        </id>  
        <property name="name" column="name" type="java.lang.String" not-null="true" length="255"/>
        <property name="type" column="type" type="java.lang.String" length="255"/>
        <property name="reason" column="reason" type="java.lang.String" length="255"/>
        <property name="dateTime" column="dateTime" type="java.lang.String" length="255"/>
        <property name="duration" column="duration" type="float" length="10"/>
        <property name="status" column="status" type="int" length="10"/>

        
    </class>  
</hibernate-mapping>